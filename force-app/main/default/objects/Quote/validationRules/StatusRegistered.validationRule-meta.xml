<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>StatusRegistered</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
    $RecordType.DeveloperName = &quot;Links&quot;, 
    ISBLANK(ProtocolNumber__c), 				
    ISPICKVAL(Status, &quot;2&quot;),
    ISPICKVAL(PRIORVALUE( Status ),&quot;1&quot;)
)</errorConditionFormula>
    <errorMessage>Prima di passare allo stato &quot;Registered&quot;, inserire il Protocol Number fornito dall&apos;amministrazione.</errorMessage>
</ValidationRule>
